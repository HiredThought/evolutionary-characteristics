{
    "name": "evolve",
    "version": "0.1.0",
    "description": "A Wardley Mapping microsite.",
    "main": "index.js",
    "dependencies": {},
    "devDependencies": {
        "js-beautify": "^1.8.9",
        "mustache": "^3.0.1",
        "npm-watch": "^0.6.0"
    },
    "watch": {
        "build": {
            "patterns": ".",
            "extensions": "json,mustache"
        }
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm run beautify && npm run render",
        "beautify": "js-beautify -r --type html *.mustache && js-beautify -r --type js *.json",
        "render": "mustache bs4.json bs4.mustache > index-bs4.html",
        "watch": "npm-watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HiredThought/evolutionary-characteristics.git"
    },
    "keywords": [
        "Wardley"
    ],
    "authors": [
        "Ben Mosior",
        "Tasshin Fogleman"
    ],
    "license": "CC-BY-SA-4.0",
    "bugs": {
        "url": "https://github.com/HiredThought/evolutionary-characteristics/issues"
    },
    "homepage": "https://github.com/HiredThought/evolutionary-characteristics#readme"
}